{
  "name": "gapstack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "nx run-many --target=lint",
    "test": "jest",
    "build": "nx run-many --target=build --production",
    "publish:imperative-render": "cd dist/packages/imperative-render && npm publish --access=public",
    "publish:light-type": "cd dist/packages/light-type && npm publish --access=public && cd - || cd -",
    "docs:base:build": "nx build-storybook docs",
    "docs:base:publish": "gh-pages -d dist/storybook/docs",
    "docs:deploy": "pnpm docs:base:build && pnpm docs:base:publish",
    "docs": "nx storybook docs",
    "light-type-benchmark:run": "pnpm build && pnpm node -r ts-node/register packages/light-type-benchmarks/run.ts",
    "light-type-benchmark:run:mac": "taskpolicy -c utility pnpm light-type-benchmark:run",
    "light-type-benchmark:run:nux": "taskset -c 1 pnpm light-type-benchmark:run",
    "tsc:check": "pnpm tsc -p tsconfig.base.json --noEmit --incremental false --jsx react-jsx --allowSyntheticDefaultImports",
    "tsc:trace": "pnpm tsc:check --generateTrace ./.trace"
  },
  "private": true,
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@nrwl/eslint-plugin-nx": "15.8.5",
    "@nrwl/jest": "15.8.5",
    "@nrwl/js": "^15.8.5",
    "@nrwl/linter": "15.8.5",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "15.8.5",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
